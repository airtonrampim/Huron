  object fdqryPedido: TFDQuery
    Connection = MainForm.fdcConnection
    SQL.Strings = (
      'SELECT Cliente.RazaoSocial,'
      '       Cliente.NomeFantasia,'
      '       Cliente.Email,'
      '       SUBSTRING(Cliente.CNPJ from 1 for 2)'
      #9'|| '#39'.'#39' || SUBSTRING(Cliente.CNPJ from 3 for 3)'
      #9'|| '#39'.'#39' || SUBSTRING(Cliente.CNPJ from 6 for 3)'
      #9'|| '#39'/'#39' || SUBSTRING(Cliente.CNPJ from 9 for 4)'
      #9'|| '#39'-'#39' || SUBSTRING(Cliente.CNPJ from 13 for 2) CNPJ,'
      '       Cliente.Telefone,'
      '       Cliente.Endereco,'
      '       Cliente.Numero,'
      '       Cliente.Bairro,'
      '       Cliente.Complemento,'
      '       Cliente.Cidade,'
      '       Cliente.Estado'
      'FROM Pedido'
      '     INNER JOIN Cliente'
      '     ON Pedido.ID_Cliente = Cliente.ID_Cliente'
      'WHERE Pedido.ID_Pedido = :ID_Pedido')
    Left = 576
    Top = 283
    ParamData = <
      item
        Name = 'ID_PEDIDO'
        DataType = ftInteger
        ParamType = ptInput
      end>
